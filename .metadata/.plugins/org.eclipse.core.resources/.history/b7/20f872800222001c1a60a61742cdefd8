public class FizzBuzz {
	public static void main(String args[]) {
		fizzBuzz(15);
	}

	public static void fizzBuzz( int a ) {
		for( int i=1; i <= a ; i++) {
			if ( i % 5 == 0 && i % 3 == 0 )
				System.out.println("fizzbuzz");
			else if ( i % 5 ==0 )
				System.out.println("buzz");
			else if ( i % 3 == 0 )
				System.out.println("fizz");
			else 
				System.out.println(i);
		}
	}
}
