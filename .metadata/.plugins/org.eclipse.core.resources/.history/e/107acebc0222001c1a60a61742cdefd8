public class FizzBuzz {
	public static void main(String args[]) {
		fizzBuzz(15);
	}

	public static void fizzBuzz( int num ) {
	      String a="";
	      for( int i=1; i <= num ; i++) {
			       	if ( i % 5 == 0 && i % 3 == 0 )
					        a+="fizzbuzz";
				else if ( i % 5 ==0 )
					  a+="buzz";
				else if ( i % 3 == 0 )
					a+="fizz";
				else 
					a+=i;
			}
	        return a;
	    
	}
}
